{
  "name": "nxjt",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Quickly navigate to npmx common webpages.",
  "author": "trueberryless <trueberryless@gmail.com> (https://trueberryless.org)",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest",
    "lint": "prettier -c --cache . && eslint . --cache --max-warnings=0 && tsc --noEmit && astro check",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/check": "0.9.6",
    "@astrojs/netlify": "^6.6.4",
    "@astrojs/vercel": "9.0.4",
    "@unocss/reset": "0.58.5",
    "astro": "5.17.1"
  },
  "devDependencies": {
    "@hideoo/eslint-config": "3.0.1",
    "@hideoo/prettier-config": "2.0.0",
    "@hideoo/tsconfig": "2.0.1",
    "@types/node": "18.19.18",
    "eslint": "8.57.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "typescript": "5.3.3",
    "vercel": "33.5.2",
    "vitest": "1.3.1"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.15.3",
  "private": true,
  "keywords": [
    "npmx",
    "nxjt",
    "npmxpmx-jump-tp",
    "chrome",
    "firefox",
    "alfred"
  ],
  "homepage": "https://github.com/trueberryless-org/nxjt",
  "repository": {
    "type": "git",
    "url": "https://github.com/trueberryless-org/nxjt.git"
  },
  "bugs": "https://github.com/trueberryless-org/nxjt/issues",
  "lint-staged": {
    "*": [
      "prettier -w -u --cache",
      "eslint --cache --no-warn-ignored --max-warnings=0"
    ]
  }
}
